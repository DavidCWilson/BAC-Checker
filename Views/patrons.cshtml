@{
  Layout="Shared/_Layout.cshtml";
}
<div class="patrons_page">

@if (@Model.Count != 0)
{
  <ul>
    @foreach (var patron in @Model)
    {
      <li><a href="/patrons/@patron.GetId()"> @patron.GetName()</a></li>
    }
  </ul>
}
else
{
  <p>AINT NOBODY NOWHERE Y'ALL</p>
}
</div>

<a href="/patrons/add"><button type="submit" class="btn btn-submit">Add a Patron</button></a>
