@{
  Layout="Shared/_Layout.cshtml";
}

@if (@Model.Count != 0)
{
  <ul>
    @foreach (var bartender in @Model)
    {
      <li><a href="/bartenders/@bartender.GetId()">@bartender.GetName()</a></li>
    }
  </ul>
}
else
{
  <p>AINT NOBODY NOWHERE Y'ALL</p>
}

<a href="/bartenders/add"><button type="button" class="bartenders-button btn btn-submit">Add a Bartender</button></a>
